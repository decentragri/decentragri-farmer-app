Auth="*res://HTTP/Auth.gd"
User="*res://HTTP/User.gd"
Utils="*res://HTTP/utils.gd"
GpsLocator="*res://addons/GPSLocator/gps_locator.gd"
Insight="*res://HTTP/Insight.gd"
Onchain="*res://HTTP/Onchain.gd"
Weather="*res://HTTP/Weather.gd"
BiometricAuth="*res://addons/BiometricAuth/biometric_auth.gd"
NetworkState="*res://addons/AndroidInternetConnectionStatePlugin/AndroidNetworkState.gd"
RealmDB="*res://addons/RealmDB/realm_db.gd"
Camera="*res://addons/GodotGetImage/Camera.gd"
Farm="*res://HTTP/Farm.gd"
Firebase="*res://addons/Firebase/firebase.gd"
